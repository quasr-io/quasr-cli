{
  "display": "Node LTS (22)",
  "_version": "22.0.0",
  "compilerOptions": {
    "lib": [
      "es2023"
    ],
    "outDir": "build",
    "module": "nodenext",
    "target": "es2022",
    "strict": true,
    "noUncheckedIndexedAccess": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "moduleResolution": "nodenext",
    "types": [
      "node"
    ]
  },
  "include": [
    "src"
  ],
  "watchOptions": {
    "watchFile": "fixedPollingInterval"
  }
}